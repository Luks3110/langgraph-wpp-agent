# Google AI API
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Server
AGENT_SERVICE_PORT=3002

# Redis Configuration
