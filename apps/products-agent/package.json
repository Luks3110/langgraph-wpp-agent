{
    "name": "@products-monorepo/products-agent",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node --loader ts-node/esm src/index.ts",
        "dev": "tsx watch src/index.ts",
        "clean": "rimraf dist",
        "lint": "eslint \"src/**/*.ts\"",
        "seed": "tsx src/utils/seed-data.ts",
        "test:agent": "tsx src/utils/test-agent.ts",
        "start:pm2": "pm2 start ecosystem.config.cjs",
        "stop:pm2": "pm2 stop ecosystem.config.cjs",
        "reload:pm2": "pm2 reload ecosystem.config.cjs",
        "monit:pm2": "pm2 monit"
    },
    "dependencies": {
        "@google/generative-ai": "latest",
        "@langchain/community": "^0.3.33",
        "@langchain/core": "^0.3.42",
        "@langchain/google-genai": "latest",
        "@langchain/langgraph": "^0.2.51",
        "@langchain/openai": "^0.4.4",
        "@langchain/qdrant": "^0.1.1",
        "@products-monorepo/shared": "workspace:*",
        "@qdrant/js-client-rest": "latest",
        "bullmq": "latest",
        "dotenv": "latest",
        "express": "latest",
        "ioredis": "latest",
        "pm2": "^5.3.1",
        "zod": "latest"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.19",
        "eslint": "^8.56.0",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18"
    }
}
